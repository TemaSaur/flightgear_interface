<?xml version="1.0"?>
<PropertyList>
	<generic>
		<output>
			<line_separator>newline</line_separator>
			<var_separator>;</var_separator>

			<!-- Get aircraft speed -->
			<chunk>
				<name>airspeed-kt</name>
				<type>float</type>
				<node>/velocities/airspeed-kt</node>
				<format>%f</format>
			</chunk>

			<chunk>
				<name>track-magnetic-deg</name>
				<type>float</type>
				<node>/orientation/track-magnetic-deg</node>
				<format>%f</format>
			</chunk>

		</output>
	</generic>
</PropertyList>
